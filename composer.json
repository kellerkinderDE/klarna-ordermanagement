{
    "name": "bestit/klarna-ordermanagement",
    "description": "Klarna ordermanagement as a package for our Klarna plugins.",
    "keywords": ["klarna", "ordermanagement", "shopware", "sw"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.2",
        "symfony/polyfill-mbstring": "1.18.1",
        "symfony/polyfill-intl-normalizer": "1.18.1",
        "symfony/polyfill-intl-grapheme": "1.18.1",
        "symfony/property-info": "v5.1.0",
        "phpdocumentor/reflection-docblock": "v5.1.0"
    },
    "autoload": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "shopware/shopware": "5.7.0-RC1"
    }
}
