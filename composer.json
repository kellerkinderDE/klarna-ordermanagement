{
    "name": "bestit/klarna-ordermanagement",
    "description": "Klarna ordermanagement as a package for our Klarna plugins.",
    "keywords": ["klarna", "ordermanagement", "shopware", "sw"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/property-info": "v3.2.14",
        "phpdocumentor/reflection-docblock": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "shopware/shopware": "5.4.0"
    }
}
