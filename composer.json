{
    "name": "k10r/klarna-ordermanagement",
    "description": "Klarna ordermanagement as a package for our Klarna plugins.",
    "keywords": ["klarna", "ordermanagement", "shopware", "sw"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.2 <=8.1"
    },
    "require-dev": {
        "k10r/codestyle": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestitKlarnaOrderManagement\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix"
    }
}
